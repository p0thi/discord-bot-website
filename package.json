{
  "name": "discord-bot-website",
  "version": "0.1.13",
  "private": true,
  "description": "Manager for the Pothi-Bot on discord",
  "author": "Pascal Pothmann",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "cross-env VUE_APP_ELECTRON_ENV=true vue-cli-service electron:build",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:release": "cross-env VUE_APP_ELECTRON_ENV=true vue-cli-service electron:build -p always",
    "electron:serve": "cross-env VUE_APP_ELECTRON_ENV=true vue-cli-service electron:serve",
    "lint:fix": "vue-cli-service lint --fix",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^4.0.1",
    "chroma-js": "^2.1.2",
    "core-js": "^3.6.4",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.2.5",
    "get-image-colors": "^2.0.0",
    "js-file-download": "^0.4.11",
    "moment": "^2.24.0",
    "node-loader": "2.0.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "sass-loader": "10.1.1",
    "shortcuts": "^1.8.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.3.0",
    "vuetify-audio": "^0.3.2",
    "vuetify-confirm": "^2.0.2",
    "vuetify-toast-snackbar": "^0.6.1",
    "vuex": "^3.1.2",
    "vuex-electron": "^1.0.3",
    "vuex-persist": "^2.2.0",
    "wavesurfer.js": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0-beta.3",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.3",
    "@vue/cli-service": "~5.0.0-beta.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "cross-env": "^7.0.2",
    "electron": "^15.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^1.0.2",
    "eslint": "^7.20.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.2.1",
    "sass": "~1.32.12",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
